# Nvtouch files
(/system)?/vendor/bin/nvtouch_daemon	u:object_r:nvtouch_exec:s0

# Nvtouch
/dev/nvtouch		u:object_r:touch_device:s0
/dev/nvtouch_dta_admin	u:object_r:nv_touch_dta_admin_device:s0
/dev/nvtouch_dta				u:object_r:nv_touch_dta_device:s0
/sys/devices/virtual/nvtouch/nvtouch/driver_mode	u:object_r:sysfs_nv_touch_mode:s0

# Nvtouch data
/data/nvtouch(/.*)?			u:object_r:nv_touch_data_file:s0
